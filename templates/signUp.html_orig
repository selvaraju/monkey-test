 </head>
 
 <body background="static/css/monkey.jpg" width="100%">
 
     <div class="container">
 
         <form class="form-signin" action="/signUp" method="post" role="form">
             <h1 class="form-info">CHAOS MONKEY </h1>
             clouds:<br>
             <select name="clouds[0]name">
               <option>dal1</option>
               <option>dal2</option>
               <option>dal3</option>
               <option>dal4</option>
             </select>
             <br>
             <br>
             vmname:<br>
             <input type="text" name="clouds[0]vmname">
             <br>
             <br>
             service:<br>
             <select name="clouds[0]service">
                 <option>rabbit</option>
                 <option>galera</option>
             </select>
             <br>
             <br>
             action:<br>
             <select name="clouds[0]action">
                 <option>start</option>
                 <option>stop</option>
                 <option>status</option>
             </select>
             <br>
             <br>
             cpumem:<br>
             <select name="clouds[0]cpumem">
                 <option>cpu</option>
                 <option>mem</option>
             </select>
             <br>
             <br>
             controller:<br>
             <input type="text" name="clouds[0]controller">
             <br>
             <br>
             comp:<br>
             <select name="clouds[0]comp">
                 <option>rabbit</option>
                 <option>galera</option>
             </select>
             <br>
             <br>
 
             <button class="btn btn-lg btn-primary" type="button">Test</button>

         </form>
      
